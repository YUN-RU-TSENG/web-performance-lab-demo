{
    "name": "vue-boilerplate",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "format": "prettier -w .",
        "lint": "eslint \"src/**/*.{js,vue}\"",
        "lint:fix": "eslint \"src/**/*.{js,vue}\" --fix",
        "lighthouse:dev": "export NODE_ENV=dev PUPPETEER_EXPERIMENTAL_CHROMIUM_MAC_ARM=true && node test/performance/index.js ",
        "lighthouse:prod": "export NODE_ENV=prod && node test/performance/index.js",
        "prepare": "husky install"
    },
    "engines": {
        "node": "v18.5.0",
        "npm": "8.12.1"
    },
    "dependencies": {
        "chart.js": "^4.2.1",
        "dayjs": "^1.11.7",
        "lodash-es": "^4.17.21",
        "pinia": "^2.0.35",
        "vue": "^3.2.47",
        "vue-chartjs": "^5.2.0",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.1.0",
        "autoprefixer": "^10.4.14",
        "cssnano": "^6.0.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-vue": "^9.10.0",
        "husky": "^8.0.0",
        "lighthouse": "^10.1.1",
        "lint-staged": "^13.2.0",
        "postcss-nested": "^6.0.1",
        "prettier": "2.8.7",
        "puppeteer": "^18.2.1",
        "rollup-plugin-visualizer": "^5.9.0",
        "unplugin-vue-components": "^0.24.1",
        "vite": "^4.2.0"
    }
}
